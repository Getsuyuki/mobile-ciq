<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.r2ciq.zq.mobileciq" >

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Splash"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CIQAuth"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".HomePage"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".testmain"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".RiskFactor"
            android:label="Mobile CIQ">
        </activity>
        <activity
            android:name=".EvolutionModel"
            android:label="Mobile CIQ">
        </activity>
        <receiver
            android:enabled="true"
            android:name=".StudioSwitcher"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="SELECTSTUDIO"></action>
            </intent-filter>
        </receiver>
    </application>

</manifest>
